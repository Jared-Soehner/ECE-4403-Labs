public class demo {
    public static void main(String[] args) {
        // Read the WavFile from a file
        WavFile wavFile = WavFile.read("anExampleFile.wav");

        // Apply different operations to the segments using Visitors
        NoiseReduce noiseReducer = new NoiseReduce();
        wavFile.accept(noiseReducer);

        Reverb reverbAdder = new Reverb();
        wavFile.accept(reverbAdder);

        Normalizer normalizer = new Normalizer();
        wavFile.accept(normalizer);
    }
}
